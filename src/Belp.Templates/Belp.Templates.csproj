<Project Sdk="Belp.SDK.PackOnly/0.6.0">

  <!-- Package Configuration -->
  <PropertyGroup>
    <IncludeContentInContentFiles>false</IncludeContentInContentFiles>

    <!-- Package Information -->
    <Description>Provides templates for MSBuild projects and other files.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/Arthri/Belp.Templates/</PackageProjectUrl>
    <PackageTags>msbuild;template;templates</PackageTags>
    <PackageType>Template</PackageType>
    <Product>Belp</Product>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <Title>Belp Templates</Title>
  </PropertyGroup>

  <ItemGroup>
    <CopyrightOwner Include="Arthri" Years="2022, 2023, 2024" />
  </ItemGroup>

</Project>
